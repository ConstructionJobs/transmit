{
    "name": "navjobs/laravel-api",
    "description": "A collection of Laravel api base classes and helpers.",
    "keywords": [
        "spatie",
        "laravel-fractal",
        "laravel",
        "fractal",
        "api",
        "transform"
    ],
    "homepage": "https://kimmel.com",
    "license": "MIT",
    "require": {
        "php" : ">=5.5.0",
        "league/fractal": "^0.13.0",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0",
        "scrutinizer/ocular": "^1.1",
        "orchestra/testbench": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "NavJobs\\LaravelApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NavJobs\\LaravelApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
